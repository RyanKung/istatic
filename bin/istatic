#!/usr/bin/env node
var istatic = require('../istatic')
  , logger = Object.create(console)
  , args = process.argv.slice(2)[0]

switch (args) {
  case 'pu':
  case 'pull':
    istatic.pull()
    break
  case '-V':
  case '--version':
    logger.info(istatic.version)
    break
  case 'clear':
    istatic.clear()
    break
}
